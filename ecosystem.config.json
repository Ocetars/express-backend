{
  "name": "mihomo-express-backend",
  "script": "dist/app.js",
  "instances": "max",
  "exec_mode": "cluster",
  "env": {
    "NODE_ENV": "development",
    "PORT": 3000
  },
  "env_production": {
    "NODE_ENV": "production",
    "PORT": 3000
  },
  "log_date_format": "YYYY-MM-DD HH:mm Z",
  "error_file": "./logs/err.log",
  "out_file": "./logs/out.log",
  "log_file": "./logs/combined.log",
  "time": true
}